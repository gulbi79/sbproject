-- SUNG.TB_MST_CODE
CREATE TABLE SUNG.TB_MST_CODE (
	GROUP_CD VARCHAR2(100) NOT NULL,
	GROUP_NM VARCHAR2(100) NOT NULL,
	CODE_CD VARCHAR2(100) NOT NULL,
	CODE_NM VARCHAR2(100) NOT NULL,
	SORT VARCHAR2(100) NULL,
	USE_YN VARCHAR2(100) NULL,
	CONSTRAINT TB_MST_CODE_PK PRIMARY KEY (GROUP_CD,CODE_CD)
);
COMMENT ON TABLE SUNG.TB_MST_CODE IS '공통코드';


-- SUNG.TB_MST_SALES
CREATE TABLE SUNG.TB_MST_SALES (
	SALES_CD VARCHAR2(100) NOT NULL,
	PARENTSALES_CD VARCHAR2(100) NULL,
	AP2_CD VARCHAR2(100) NULL,
	AP1_CD VARCHAR2(100) NULL,
	ACCOUNT_CD VARCHAR2(100) NULL,
	LEVEL_CD VARCHAR2(100) NULL,
	CONSTRAINT TB_MST_SALES_PK PRIMARY KEY (SALES_CD)
);


-- SUNG.TB_MST_PRODUCT
CREATE TABLE SUNG.TB_MST_PRODUCT (
	ITEM_CD VARCHAR2(100) NOT NULL,
	PRODUCTGROUP_CD VARCHAR2(100) NULL,
	PRODUCT_CD VARCHAR2(100) NULL,
	PARENT_CD VARCHAR2(100) NULL,
	LEVEL_CD VARCHAR2(100) NULL,
	CONSTRAINT TB_MST_PRODUCT_PK PRIMARY KEY (ITEM_CD)
);

